{{
$type = $1.type | type_to_string

$value = $1.value;

if string.ends_with $type 'Flags'
    $value = '(' + $type + ')' + $value
else if $type == 'string'
    $value = '"' + $value + '"'
end
}}        
        {{~ if $1.doc && $1.doc.text ~}}
        /// <summary>
        {{~ $1.doc.text | comment_line_by_line_with_prefix '\t\t'~}}
        /// </summary>
        {{ end }}
        public static {{ $type }} {{ $1.name | fix_identifier }} = {{ $value }};
